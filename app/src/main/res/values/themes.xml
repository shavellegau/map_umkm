<resources xmlns:tools="http://schemas.android.com/tools">
    <style name="Theme.Map_UMKM" parent="Theme.Material3.DayNight.NoActionBar">

        <item name="colorPrimary">@color/tuku_primary</item>
        <item name="colorPrimaryVariant">@color/tuku_dark</item>
        <item name="colorOnPrimary">@color/white</item>

        <item name="android:statusBarColor">@android:color/transparent</item>
        <item name="android:windowLightStatusBar">true</item>
    </style>

    <style name="ToolbarTitleAppearance" parent="TextAppearance.Widget.AppCompat.Toolbar.Title">
        <item name="android:textSize">20sp</item>
        <item name="android:textStyle">bold</item>
    </style>

    <style name="RoundedImageView" parent="">
        <item name="cornerFamily">rounded</item>
        <item name="cornerSize">16dp</item>
    </style>

    <style name="ShapeAppearanceOverlay.App.CornerSize10Percent" parent="">
        <item name="cornerFamily">rounded</item>
        <item name="cornerSize">10%</item>
    </style>

    <style name="CustomCorners" parent="">
        <item name="cornerFamily">rounded</item>
        <item name="cornerSize">10dp</item>
    </style>

</resources>